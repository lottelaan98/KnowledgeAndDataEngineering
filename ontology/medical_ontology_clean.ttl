  skos:prefLabel "Typhoid fever"@en ;
  ex:hasPrimarySymptom ex:symptom/fever ;
  ex:hasPrimarySymptom ex:symptom/diarrhea ;
  ex:hasSecondarySymptom ex:symptom/headache ;
  ex:hasSecondarySymptom ex:symptom/fatigue .

ex:disease/chickenpox a ex:Disease ;
  skos:prefLabel "Chickenpox"@en ;
