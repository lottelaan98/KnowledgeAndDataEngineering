@prefix ex: <http://uu.nl/medical/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

############################
# Classes
############################

ex:Disease a owl:Class .
ex:Symptom a owl:Class .

############################
# Properties
############################

ex:hasPrimarySymptom a owl:ObjectProperty .
ex:hasSecondarySymptom a owl:ObjectProperty .
ex:hasComplication a owl:ObjectProperty .

############################
# Symptoms
############################

ex:symptom/fever a ex:Symptom ; skos:prefLabel "fever"@en .
ex:symptom/headache a ex:Symptom ; skos:prefLabel "headache"@en .
ex:symptom/fatigue a ex:Symptom ; skos:prefLabel "fatigue"@en .
ex:symptom/pain a ex:Symptom ; skos:prefLabel "pain"@en .
ex:symptom/cough a ex:Symptom ; skos:prefLabel "cough"@en .
ex:symptom/dyspnea a ex:Symptom ; skos:prefLabel "shortness of breath"@en .
ex:symptom/rash a ex:Symptom ; skos:prefLabel "rash"@en .
ex:symptom/itching a ex:Symptom ; skos:prefLabel "itching"@en .
ex:symptom/joint-pain a ex:Symptom ; skos:prefLabel "joint pain"@en .
ex:symptom/nausea a ex:Symptom ; skos:prefLabel "nausea"@en .
ex:symptom/vomiting a ex:Symptom ; skos:prefLabel "vomiting"@en .
ex:symptom/diarrhea a ex:Symptom ; skos:prefLabel "diarrhea"@en .
ex:symptom/chest-pain a ex:Symptom ; skos:prefLabel "chest pain"@en .
ex:symptom/yellow-skin a ex:Symptom ; skos:prefLabel "yellow skin"@en .
ex:symptom/yellow-eyes a ex:Symptom ; skos:prefLabel "yellow eyes"@en .
ex:symptom/frequent-urination a ex:Symptom ; skos:prefLabel "frequent urination"@en .
ex:symptom/blurred-vision a ex:Symptom ; skos:prefLabel "blurred vision"@en .

############################
# Diseases
############################

ex:disease/psoriasis a ex:Disease ;
  skos:prefLabel "Psoriasis"@en ;
  ex:hasPrimarySymptom ex:symptom/rash ;
  ex:hasSecondarySymptom ex:symptom/itching ;
  ex:hasComplication ex:symptom/joint-pain .

ex:disease/varicose-veins a ex:Disease ;
  skos:prefLabel "Varicose veins"@en ;
  ex:hasPrimarySymptom ex:symptom/pain ;
  ex:hasSecondarySymptom ex:symptom/fatigue .

