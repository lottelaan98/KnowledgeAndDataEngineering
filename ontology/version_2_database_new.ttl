@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix wd:   <http://www.wikidata.org/entity/> .
@prefix ex:   <http://uu.nl/medical/> .

#################################################################
# CLASSES
#################################################################

ex:Disease a owl:Class ;
  rdfs:label "Disease"@en ;
  owl:equivalentClass wd:Q12136 .

ex:Symptom a owl:Class ;
  rdfs:label "Symptom"@en ;
  owl:equivalentClass wd:Q169872 .

#################################################################
# SYMPTOM SUBCLASSES
#################################################################

ex:SkinSymptom a owl:Class ; rdfs:subClassOf ex:Symptom .
ex:GastrointestinalSymptom a owl:Class ; rdfs:subClassOf ex:Symptom .
ex:RespiratorySymptom a owl:Class ; rdfs:subClassOf ex:Symptom .
ex:NeurologicalSymptom a owl:Class ; rdfs:subClassOf ex:Symptom .
ex:CardiovascularSymptom a owl:Class ; rdfs:subClassOf ex:Symptom .
ex:SystemicSymptom a owl:Class ; rdfs:subClassOf ex:Symptom .

#################################################################
# PROPERTIES
#################################################################

ex:hasPrimarySymptom a owl:ObjectProperty ;
  rdfs:domain ex:Disease ;
  rdfs:range ex:Symptom .

ex:hasSecondarySymptom a owl:ObjectProperty ;
  rdfs:domain ex:Disease ;
  rdfs:range ex:Symptom .

ex:hasComplication a owl:ObjectProperty ;
  rdfs:domain ex:Disease ;
  rdfs:range ex:Symptom .

ex:definition a owl:DatatypeProperty ;
  rdfs:domain ex:Disease ;
  rdfs:range rdf:langString .

#################################################################
# SYMPTOMS (NORMALIZED CORE SET)
#################################################################

ex:symptom-pain a ex:SystemicSymptom ; skos:prefLabel "pain"@en ; skos:exactMatch wd:Q81938 .
ex:symptom-fatigue a ex:SystemicSymptom ; skos:prefLabel "fatigue"@en ; skos:exactMatch wd:Q9690 .
ex:symptom-fever a ex:SystemicSymptom ; skos:prefLabel "fever"@en ; skos:exactMatch wd:Q38933 .
ex:symptom-headache a ex:NeurologicalSymptom ; skos:prefLabel "headache"@en ; skos:exactMatch wd:Q86 .
ex:symptom-nausea a ex:GastrointestinalSymptom ; skos:prefLabel "nausea"@en ; skos:exactMatch wd:Q186889 .
ex:symptom-vomiting a ex:GastrointestinalSymptom ; skos:prefLabel "vomiting"@en ; skos:exactMatch wd:Q127076 .
ex:symptom-diarrhea a ex:GastrointestinalSymptom ; skos:prefLabel "diarrhea"@en ; skos:exactMatch wd:Q40878 .
ex:symptom-cough a ex:RespiratorySymptom ; skos:prefLabel "cough"@en ; skos:exactMatch wd:Q35805 .
ex:symptom-dyspnea a ex:RespiratorySymptom ; skos:prefLabel "shortness of breath"@en ; skos:exactMatch wd:Q188008 .
ex:symptom-rash a ex:SkinSymptom ; skos:prefLabel "rash"@en ; skos:exactMatch wd:Q653197 .
ex:symptom-itching a ex:SkinSymptom ; skos:prefLabel "itching"@en ; skos:exactMatch wd:Q199602 .
ex:symptom-joint-pain a ex:SystemicSymptom ; skos:prefLabel "joint pain"@en ; skos:exactMatch wd:Q683498 .
ex:symptom-chest-pain a ex:CardiovascularSymptom ; skos:prefLabel "chest pain"@en ; skos:exactMatch wd:Q693058 .
ex:symptom-rapid-heart-rate a ex:CardiovascularSymptom ; skos:prefLabel "rapid heart rate"@en ; skos:exactMatch wd:Q192304 .

