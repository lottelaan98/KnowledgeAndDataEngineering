# diabetes (symptoms wikidata: polyuria, polydipsia, polyphagia)
CONSTRUCT {
    wd:Q12206 a ex:Disease ;
        skos:prefLabel ?dLabel ;
        ex:hasSymptom ?symptom .

    ?symptom a ex:Symptom ;
        skos:prefLabel ?sLabel .
}
WHERE {
    wd:Q12206 rdfs:label ?dLabel .
    FILTER(lang(?dLabel) = "en")

    OPTIONAL {
        wd:Q12206 wdt:P780 ?symptom .
        ?symptom rdfs:label ?sLabel .
        FILTER(lang(?sLabel) = "en")
  }
}

