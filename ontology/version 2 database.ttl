PREFIX wd:  <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos:<http://www.w3.org/2004/02/skos/core#>
PREFIX ex:  <http://example.org/med#>


# Classes
ex:Disease a owl:Class ;
    rdfs:label "Disease"@en .

ex:Symptom a owl:Class ;
    rdfs:label "Symptom"@en .

# ex:SkinSymptom a owl:Class ;
#    rdfs:label "Skin symptom" ;
#    rdfs:subClassOf ex:Symptom .

ex:Explanation a owl:Class ;
    rdfs

# Object properties
ex:hasSymptom a owl:ObjectProperty ;
    rdfs:domain ex:Disease ;
    rdfs:range ex:Symptom .

ex:hasExplanation a owl:ObjectProperty ;
    rdfs:domain ex:Disease ;
    rdfs:range ex:Explanation .




