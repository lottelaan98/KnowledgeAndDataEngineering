# Psoriasis (symptoms wikidata: no results)
CONSTRUCT {
    wd:Q179945 a ex:Disease ;
        skos:prefLabel ?dLabel ;
        ex:hasSymptom ?symptom .

    ?symptom a ex:Symptom ;
        skos:prefLabel ?sLabel .
}
WHERE {
    wd:Q179945 rdfs:label ?dLabel .
    FILTER(lang(?dLabel) = "en")

    OPTIONAL {
        wd:Q179945 wdt:P780 ?symptom .
        ?symptom rdfs:label ?sLabel .
        FILTER(lang(?sLabel) = "en")
  }
}

wd:Q179945 ex:hasExplanation "Psoriasis is commonly associated with fatigue.",
        "Psoriasis is commonly associated with pain." ;
    ex:hasSymptom ex:symptom/red_inflamed_skin_patches, 
        wd:Q9690, # fatigue
        wd:Q81938, # pain
        wd:Q29971784, # nail pitting
        wd:Q102187111, # nail discoloration
        ex:symptom/joint_pain, 
        wd:Q2971524 . # eye inflammation


