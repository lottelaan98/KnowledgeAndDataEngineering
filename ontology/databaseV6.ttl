### ALARM SYMTPOMS ###

ex:alarmMessage ex:alarm "Please contact your GP Immediately!"@en .

sym:high_temprature a ex:AlarmSymptom ;
    rdfs:label "high body temprature"@en ;
    skos:closeMatch wd:Q38933 ;         # fever
    ex:treshold "40"^^xsd:decimal .

sym:extreme_pain a ex:AlarmSymptom ;
    rdfs:label "extreme pain"@en ;
    skos:closeMatch wd:Q81938 ;            # pain
    ex:treshold "9"^^xsd:decimal . 

sym:high_blood_pressure a ex:AlarmSymptom ;
    rdfs:label "high blood pressure" ;
    ex:treshold "180"^^xsd:decimal . 


